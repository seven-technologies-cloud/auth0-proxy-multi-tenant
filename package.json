{
  "name": "auth0-proxy-api",
  "version": "1.0.0",
  "description": "Auth0 Multi-Tenancy Management API - A proxy API for managing Auth0 tenants and users with seat-based licensing",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "node node_modules/jest/bin/jest.js",
    "test:watch": "node node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node node_modules/jest/bin/jest.js --coverage",
    "test:unit": "node node_modules/jest/bin/jest.js tests/unit",
    "test:integration": "node node_modules/jest/bin/jest.js tests/integration",
    "test:security": "node node_modules/jest/bin/jest.js tests/security",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:security",
    "test:verbose": "node node_modules/jest/bin/jest.js --verbose",
    "test:debug": "node node_modules/jest/bin/jest.js --detectOpenHandles --forceExit",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "validate": "npm run lint && npm run test",
    "install:deps": "npm install",
    "setup": "npm run install:deps && cp .env.example .env",
    "test:e2e": "NODE_ENV=test MOCK_AUTH0_API=false node node_modules/jest/bin/jest.js --config jest.e2e.config.js"
  },
  "keywords": [
    "auth0",
    "multi-tenancy",
    "api",
    "proxy",
    "nodejs",
    "express",
    "jwt",
    "management"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^5.1.0",
    "express-rate-limit": "^7.4.1",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "auth0": "^4.28.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "express-jwt": "^8.4.1",
    "joi": "^17.13.3",
    "winston": "^3.14.2",
    "axios": "^1.7.7",
    "compression": "^1.7.4",
    "express-validator": "^7.2.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.4",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "nock": "^13.5.4",
    "jest-extended": "^4.0.2",
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "babel-jest": "^29.7.0",
    "eslint": "^9.11.1",
    "prettier": "^3.3.3",
    "@types/jest": "^29.5.13"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/auth0-proxy-api.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/auth0-proxy-api/issues"
  },
  "homepage": "https://github.com/your-username/auth0-proxy-api#readme"
}